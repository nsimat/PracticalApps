@page
@model NorthwindWeb.Pages.FournisseursModel
<div class="row">
    <h1 class="display-2">Fournisseurs</h1>
    <table class="table">
        <thead class="thead-inverse">
            <tr><th>Nom de compagnie</th></tr>
        </thead>
        <tbody>
            @foreach (string name in Model.Fournisseurs)
            {
                <tr><td>@name</td></tr>
            }
        </tbody>
    </table>
</div>